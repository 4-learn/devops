---
- hosts: localhost
  name: beta-eid signin
  tasks:
    - name: API request
      uri:
        method: POST
        url: https://beta-eid-backend.townway.com.tw/accounts/signin
        body_format: form-multipart
        body:
          email: "ntc@gmail.com"
          password: ${{ ntcpw }} 
        status_code: 200
      register: result
      failed_when: result.status not in [200]
...
